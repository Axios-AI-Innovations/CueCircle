{
  "expo": {
    "name": "CueCircle",
    "slug": "cuecircle",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": "2.0.0",
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      "expo-notifications",
      "expo-background-fetch",
      "expo-task-manager"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
